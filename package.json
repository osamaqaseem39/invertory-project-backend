{
  "name": "user-management-system",
  "version": "1.0.0",
  "description": "Production-ready User Management System with RBAC",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:server": "tsx watch src/server.ts",
    "start:clean": "bash scripts/start-server.sh",
    "stop": "bash scripts/stop-server.sh",
    "build": "tsc",
    "start": "node dist/server.js",
    "vercel-build": "npm run build && npm run db:migrate:prod && npm run db:seed:prod",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:seed": "tsx src/seed.ts",
    "db:seed:prod": "node dist/seed.js",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "user-management",
    "rbac",
    "auth",
    "jwt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "@types/cors": "^2.8.19",
    "@types/handlebars": "^4.0.40",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "argon2": "^0.44.0",
    "colord": "^2.9.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "groq-sdk": "^0.33.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^6.3.0",
    "pdfjs-dist": "^5.4.296",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.4",
    "tesseract.js": "^6.0.1",
    "uuid": "^10.0.0",
    "wcag-contrast": "^3.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "prisma": "^5.19.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  }
}
